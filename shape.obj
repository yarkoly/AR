mtllib shape.mtl
o Cube
v -1 -1 1
v -1 1 1
v 1 -1 1
v 1 1 1
v 1 -1 -1
v 1 1 -1
v -1 -1 -1
v -1 1 -1
# 8 vertices

vt 0.00000 0.00000
vt 0.00000 1.00000
vt 1.00000 1.00000
vt 1.00000 0.00000
# 4 texture coordinates

vn -0 -0 -1
vn -1 -0 0
vn -0 -0 1
vn 1 -0 0
vn -0 -1 0
vn -0 1 0
# 6 vertex normals

usemtl Mat
f 1/1/1 2/2/1 4/3/1 3/4/1
f 3/1/2 4/2/2 6/3/2 5/4/2
f 5/1/3 6/2/3 8/3/3 7/4/3
f 7/1/4 8/2/4 2/3/4 1/4/4
f 2/1/5 8/2/5 6/3/5 4/4/5
f 7/1/6 1/2/6 3/3/6 5/4/6
# 6 facets

